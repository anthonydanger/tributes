<% provide(:title, @title) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <%= image_tag @tribute.image(:thumb) %>
      <h1><%= link_to @tribute.name, @tribute %></h1>
      <span></span>
    </section>
  </aside>
  <div class="col-md-8">
    <h3><%= @title %></h3>
    <% if @users.any? %>
      <ul>
        <% @users.each do |user| %>
         <%= user.name %>
         <%= link_to image_tag(user.avatar(:thumb)), user %>
        <% end %>
      </ul> 
      <%= will_paginate @users %>
    <% end %>
  </div>
</div>